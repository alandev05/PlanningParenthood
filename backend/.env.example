# Flask Configuration
FLASK_ENV=development
SECRET_KEY=your-secret-key-here

# Firebase Configuration
# Option 1: Use service account credentials file (recommended for development)
FIREBASE_CREDENTIALS_PATH=firebase-credentials.json

# Option 2: Use project ID with default credentials (for deployed environments)
# FIREBASE_PROJECT_ID=planningparenthood-6de94

# Option 3: Use environment variables (for production/sharing)
# FIREBASE_PROJECT_ID=planningparenthood-6de94
# FIREBASE_PRIVATE_KEY_ID=your-private-key-id
# FIREBASE_PRIVATE_KEY=your-private-key
# FIREBASE_CLIENT_EMAIL=your-client-email

# Redis Configuration (optional)
REDIS_URL=redis://localhost:6379/0

# LLM API Keys
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# Google Maps API
GOOGLE_MAPS_API_KEY=your-google-maps-api-key

# Fetch.ai Configuration
FETCHAI_ENABLED=false
FETCHAI_OEF_ADDR=127.0.0.1
FETCHAI_OEF_PORT=10000