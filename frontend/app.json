{
  "expo": {
    "name": "Planning Parenthood",
    "slug": "planning-parenthood",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "extra": {
      "googleMapsApiKey": "$GOOGLE_MAPS_API_KEY"
    },
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "$GOOGLE_MAPS_API_KEY"
      },
      "bundleIdentifier": "com.anonymous.planning-parenthood"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "config": {
        "googleMaps": {
          "apiKey": "$GOOGLE_MAPS_API_KEY"
        }
      }
    }
  }
}